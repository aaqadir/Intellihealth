pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building Project'
            }
        }
        stage('Run') {
            steps {
                echo 'Runniing Project'
            }
        }
        stage('Deploy') {
            steps {
                echo 'deploying project'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing Project'
            }
        }
        stage('Release') {
            steps {
                echo 'releasing project'
            }
        }
    }
}
